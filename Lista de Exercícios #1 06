var alturaTriangulo, baseTriangulo;
var areaTriangulo

alturaTriangulo = parseFloat(prompt("Insira o valor da altura do triângulo (cm): "));
baseTriangulo = parseFloat(prompt("Insira o valor da base do triângulo (cm): "));

areaTriangulo = (alturaTriangulo * baseTriangulo) / 2;

alert("A área do triângulo: " + areaTriangulo + "cm²");


